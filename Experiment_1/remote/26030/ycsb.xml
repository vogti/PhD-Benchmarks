<?xml version="1.0" encoding="UTF-8"?><parameters>
    <dbtype>postgres</dbtype>
    <driver>org.postgresql.Driver</driver>
    <DBUrl>jdbc:postgresql://black12.maas:5432/test</DBUrl>
    <username>postgres</username>
    <password>postgres</password>
    <ddlFileName>ycsb-postgres-ddl.sql</ddlFileName>
    <isolation>TRANSACTION_SERIALIZABLE</isolation>
    <batchsize>128</batchsize>
    <scalefactor>1</scalefactor>
    <terminals>1</terminals>
    <works>
        <work>
            <serial>false</serial>
            <time>60</time>
            <rate>unlimited</rate>
            <warmup>60</warmup>
            <weights>20,20,20,20,20,0</weights>
        </work>
    </works>
    <transactiontypes>
        <transactiontype>
            <name>ReadRecord</name>
        </transactiontype>
        <transactiontype>
            <name>InsertRecord</name>
        </transactiontype>
        <transactiontype>
            <name>ScanRecord</name>
        </transactiontype>
        <transactiontype>
            <name>UpdateRecord</name>
        </transactiontype>
        <transactiontype>
            <name>DeleteRecord</name>
        </transactiontype>
        <transactiontype>
            <name>ReadModifyWriteRecord</name>
        </transactiontype>
    </transactiontypes>
</parameters>
